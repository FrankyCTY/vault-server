  "scripts": {
    "pre:start": "cross-env NODE_PATH=./src",
    "dev": "yarn pre:start && ./node_modules/.bin/nodemon ./src/index.js --exec babel-node index",
    "start": "nod ./src/index.js",
    "create:migration": "mikro-orm migration:create",
