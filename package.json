{
  "name": "vault-server",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:FrankyCTY/vault-server.git",
  "author": "frankycty <frankychantakyu@yahoo.com.hk>",
  "license": "MIT",
  "scripts": {
    "watch": "tsc -w",
    "pre:start": "cross-env NODE_PATH=./src",
    "dev": "yarn pre:start && ./node_modules/.bin/nodemon dist/index.js --exec babel-node dist/index",
    "start": "node dist/index.js",
    "start-tsnode": "ts-node src/index.ts",
    "dev-tsnode": "nodemon --exec ts-node src/index.ts",
    "create:migration": "mikro-orm migration:create",
    "bootstrap:dep": "yarn && yarn watch",
    "reboot:dep": "rm yarn.lock && rm -rf ./dist && rm -rf ./node_modules && yarn bootstrap:dep"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@types/config": "^0.0.38",
    "@types/connect-redis": "^0.0.16",
    "@types/cors": "^2.8.10",
    "@types/koa": "^2.13.3",
    "@types/koa__router": "^8.0.4",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^15.12.1",
    "@types/ramda": "^0.27.40",
    "@types/redis": "^2.8.29",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.7",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@frankycty/rm-extra": "^1.1.2",
    "@koa/router": "^10.0.0",
    "apollo-server-koa": "^2.25.1",
    "argon2": "^0.28.1",
    "class-validator": "^0.13.1",
    "config": "^3.3.6",
    "connect-redis": "^6.0.0",
    "graphql": "^15.5.0",
    "koa": "^2.13.1",
    "koa-session": "^6.2.0",
    "merge-descriptors": "^1.0.1",
    "mongoose": "^5.12.13",
    "qs": "^6.10.1",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  }
}
